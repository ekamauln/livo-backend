                },
                "initial_role": {
                    "type": "string",
                    "example": "picker"
                },
                "is_active": {
                    "type": "boolean",
                    "example": true
                },
                "password": {
                    "type": "string",
                    "minLength": 6,
                    "example": "password123"
                },
                "username": {
                    "type": "string",
                    "maxLength": 50,
                    "minLength": 3,
                    "example": "john_doe"
                }
            }
        },
